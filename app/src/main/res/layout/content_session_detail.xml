<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="digitalquantuminc.inscribesecuresms.ChildrenActivity.ActivitySessionDetail"
    tools:showIn="@layout/activity_session_detail">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/partner_name"
                android:textAlignment="viewStart" />

            <TextView
                android:id="@+id/text_PartnerName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_dummy_name"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/partner_number" />

            <TextView
                android:id="@+id/text_PartnerNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_dummy_number"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/partner_role" />

            <TextView
                android:id="@+id/text_PartnerRole"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_dummy_name"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/partner_rsa_pub_key" />

            <EditText
                android:id="@+id/text_PartnerRSAPubKey"
                android:labelFor="@+id/text_PartnerRSAPubKey"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="start"
                android:inputType="textMultiLine"
                android:maxLines="3"
                android:minLines="3"
                android:scrollbars="vertical"
                android:text="@string/dev_dummy_longtext"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/secure_session_status" />

            <TextView
                android:id="@+id/text_SecureSessionStatus"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_dummy_status"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="18sp"
                android:textStyle="bold" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/secure_session_freshness" />

            <TextView
                android:id="@+id/text_SecureSessionFreshness"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_dummy_status"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="18sp"
                android:textStyle="bold" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/last_secure_session_handshake" />

            <TextView
                android:id="@+id/text_LastSessionHandshake"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_dummy_date"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/text_SessionHandshakeAge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_dummy_duration"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/secure_session_nummessage" />

            <TextView
                android:id="@+id/text_SessionNumMessage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_dummy_number"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/ecdh_session_priv_key" />

            <EditText
                android:id="@+id/text_ECDHSessionPrivateKey"
                android:labelFor="@+id/text_ECDHSessionPrivateKey"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="start"
                android:inputType="textMultiLine"
                android:maxLines="3"
                android:minLines="3"
                android:scrollbars="vertical"
                android:text="@string/dev_dummy_longtext"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/ecdh_session_pub_key" />

            <EditText
                android:id="@+id/text_ECDHSessionPublicKey"
                android:labelFor="@+id/text_ECDHSessionPublicKey"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="start"
                android:inputType="textMultiLine"
                android:maxLines="3"
                android:minLines="3"
                android:scrollbars="vertical"
                android:text="@string/dev_dummy_longtext"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/ecdh_partner_session_pub_key" />

            <EditText
                android:id="@+id/text_PartnerECDHSessionPublicKey"
                android:labelFor="@+id/text_PartnerECDHSessionPublicKey"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="start"
                android:inputType="textMultiLine"
                android:maxLines="3"
                android:minLines="3"
                android:scrollbars="vertical"
                android:text="@string/dev_dummy_longtext"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/ecdh_partner_digsign" />

            <EditText
                android:id="@+id/text_PartnerDigitalSignature"
                android:labelFor="@+id/text_PartnerDigitalSignature"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="start"
                android:inputType="textMultiLine"
                android:maxLines="3"
                android:minLines="3"
                android:scrollbars="vertical"
                android:text="@string/dev_dummy_longtext"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/ecdh_partner_computedigsign" />

            <EditText
                android:id="@+id/text_PartnerComputedDigitalSignature"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="start"
                android:inputType="textMultiLine"
                android:labelFor="@+id/text_PartnerDigitalSignature"
                android:maxLines="3"
                android:minLines="3"
                android:scrollbars="vertical"
                android:text="@string/dev_dummy_longtext"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/ecdh_partner_validity" />

            <TextView
                android:id="@+id/text_PartnerValidity"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_dummy_status"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="18sp"
                android:textStyle="bold" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/session_shared_secret" />

            <EditText
                android:id="@+id/text_SharedSecret"
                android:labelFor="@+id/text_SharedSecret"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="start"
                android:inputType="textMultiLine"
                android:maxLines="3"
                android:minLines="3"
                android:scrollbars="vertical"
                android:text="@string/dev_dummy_longtext"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/session_aes_key" />

            <EditText
                android:id="@+id/text_SessionAESKey"
                android:labelFor="@+id/text_SessionAESKey"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="start"
                android:inputType="textMultiLine"
                android:maxLines="3"
                android:minLines="3"
                android:scrollbars="vertical"
                android:text="@string/dev_dummy_longtext"
                android:textSize="18sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <Button
                android:id="@+id/btn_InitiateSession"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/initiate_secure_session" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <Button
                android:id="@+id/btn_EndSession"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/end_secure_session" />
        </LinearLayout>
    </ScrollView>
</android.support.constraint.ConstraintLayout>
